<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.mkorakin.UiDesignPatternsByExample"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.example.mkorakin.UiDesignPatternsByExample.clickers.SimpleClicker.mvvm.SimpleClickerMvvmActivity"/>
        <activity android:name="com.example.mkorakin.UiDesignPatternsByExample.clickers.SimpleClicker.mvp.SimpleClickerMvpActivity"/>
        <activity android:name="com.example.mkorakin.UiDesignPatternsByExample.clickers.SimpleClicker.mvc.SimpleClickerMvcActivity"/>
        <activity android:name="com.example.mkorakin.UiDesignPatternsByExample.clickers.LongPressClicker.LongPressClickerActivity"/>
        <activity android:name="com.example.mkorakin.UiDesignPatternsByExample.clickers.ClickerEditor.ClickerEditorActivity"/>
        <activity android:name="com.example.mkorakin.UiDesignPatternsByExample.clickers.TwoThumbClicker.TwoThumbsClickerActivity"/>
        <activity android:name="com.example.mkorakin.UiDesignPatternsByExample.clickers.ToolbarClicker.ToolbarClickerActivity"/>
    </application>

</manifest>